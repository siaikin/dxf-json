{
  "name": "@mlightcad/dxf-json",
  "version": "1.0.4",
  "description": "forked version of dxf-json",
  "type": "module",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "import": "./dist/bundle.mjs",
    "require": "./dist/bundle.cjs"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.build.json && rollup --config",
    "test": "vitest run --config ./vite.config.ts && npm run typetest",
    "test-dev": "vitest --config ./vite.config.ts --silent=false",
    "analyze": "depcruise src --include-only \".*(types|index|consts).ts\" -T dot | dot -Grankdir=TD -Gsplines=ortho -T svg > dependency-graph.svg",
    "new": "vite-node scripts/create-snippet.ts",
    "typetest": "npm --workspace=integration-test run build",
    "print": "vite-node ./scripts/print.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlight-lee/dxf-json.git"
  },
  "keywords": [
    "dxf",
    "cad",
    "dxf-json",
    "json",
    "type-safe-dxf",
    "type-safe",
    "typescript"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "GPL-3.0",
  "homepage": "https://github.com/mlight-lee/dxf-json",
  "dependencies": {
    "@fxts/core": "^1.0.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.107",
    "@types/readable-stream": "^4.0.10",
    "@types/three": "^0.160.0",
    "dependency-cruiser": "^17.0.0",
    "rollup": "^4.9.6",
    "rollup-plugin-swc3": "^0.12.1",
    "typescript": "^5.3.3",
    "vite-node": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    ".",
    "integration-test"
  ]
}
